<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <Version>1.0.0-beta6</Version>
    <Authors>Jens Krumsieck</Authors>
    <Company>Jens Krumsieck</Company>
    <Copyright>2019-2020 Jens Krumsieck</Copyright>
    <PackageProjectUrl>https://github.com/JensKrumsieck/PorphyStruct</PackageProjectUrl>
    <PackageIcon>porphystructlogo.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/JensKrumsieck/PorphyStruct</RepositoryUrl>
    <FileVersion>1.0.0.6</FileVersion>
    <ApplicationIcon>Resources\favicon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\favicon-32x32.png" />
    <None Remove="Resources\favicon.ico" />
    <None Remove="Resources\logo.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Corrole.png" />
    <Resource Include="Resources\Corrphycene.png" />
    <Resource Include="Resources\favicon.ico" />
    <Resource Include="Resources\logo.png" />
    <Resource Include="Resources\porphystructlogo.png" />
    <Resource Include="Resources\Norcorrole.png" />
    <Resource Include="Resources\Porphycene.png" />
    <Resource Include="Resources\Porphyrin.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="HelixToolkit.Core.Wpf" Version="2.10.0" />
    <PackageReference Include="MaterialDesignThemes" Version="3.1.0-ci948" />
    <PackageReference Include="OxyPlot.Wpf" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\PorphyStruct.Core\PorphyStruct.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Language\Sim.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Sim.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Language\Main.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Main.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Language\Sim.de.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Language\Sim.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Sim.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Language\Main.de.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Language\Main.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Main.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>